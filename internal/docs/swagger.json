                    "application/toml"
                ],
                "tags": [
                    "Task"
                ],
                "summary": "task workspace delete",
                "parameters": [
                    {
                        "type": "string",
                        "description": "task name",
                        "name": "task",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "dir path",
                        "name": "path",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/types.BaseRes"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
