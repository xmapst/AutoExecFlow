                ],
                "produces": [
                    "application/json",
                    "application/x-yaml",
                    "application/toml"
                ],
                "tags": [
                    "Task"
                ],
                "summary": "task workspace list",
                "parameters": [
                    {
                        "type": "string",
                        "description": "task name",
                        "name": "task",
                        "in": "path",
